{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "identity": {
      "type": "string"
    },
    "keyVaultName":{
      "type": "string",
      "defaultValue": "\\\"gh2149-rg\\\""
    },
    "keyName":{
      "type": "string",
      "defaultValue": "\\\"des2Key\\\""
    },
    "utcValue": {
      "type": "string",
      "defaultValue": "[utcNow()]"
    },
    ""
  },
  "resources": [
    {
      "type": "Microsoft.Resources/deploymentScripts",
      "apiVersion": "2019-10-01-preview",
      "name": "CreateKeyVaultKey",
      "location": "West US",
      "kind": "AzurePowerShell",
      "identity": {
        "type": "userAssigned",
        "userAssignedIdentities": {
          "/subscriptions/9e01c1b0-6b2c-4deb-81e1-80aa08a9f985/resourceGroups/GHPI-TestInfra-Resources/providers/Microsoft.ManagedIdentity/userAssignedIdentities/ghpiEv2_AzFn_test_identity"
        }
      },
      "properties": {
        "forceUpdateTag": "[parameters('utcValue')]",
        "azPowerShellVersion": "3.0",
        "scriptContent": "
          param (
            [string]$keyVaultName,
            [string]$keyName
          )
          
          write-Output keyVaultName: $keyVaultName
          write-Output keyName: $keyName
          $key = Add-AzKeyVaultKey -VaultName $keyVaultName  -name $keyName -Destination 'Software'
          write-Output $keyName
          $DeploymentScriptOutputs = @{}
          $DeploymentScriptOutputs['keyVaultUrl'] = $key.Id
        ",
        "arguments": "[concat('-keyVaultName', ' ', parameters('keyVaultName'), ' ', '-keyName', ' ', parameters('keyName'))]",
        "timeout": "PT1H",
        "cleanupPreference": "OnSuccess",
        "retentionInterval": "P1D"
      }
    }
  ],
  "outputs": {
    "result": {
      "value": "[reference('CreateKeyVaultKey').outputs.keyVaultUrl]",
      "type": "string"
    }
  }
}
